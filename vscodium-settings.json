{
	"workbench.colorTheme": "Community Material Theme Palenight High Contrast",
	"workbench.activityBar.visible": false,
	"editor.minimap.enabled": false,
	"breadcrumbs.enabled": false,
	"security.workspace.trust.banner": "never",
	"security.workspace.trust.startupPrompt": "never",
	"security.workspace.trust.enabled": false,
	"security.workspace.trust.emptyWindow": false,
	"svelte.ask-to-enable-ts-plugin": false,
	"editor.fontFamily": "'Fira Code', 'Droid Sans Mono', 'monospace', monospace",
	"editor.fontSize": 13.6,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.foldingImportsByDefault": true,
	"editor.guides.bracketPairs": true,
	"editor.linkedEditing": true,
	"editor.scrollbar.horizontalScrollbarSize": 5,
	"editor.scrollbar.verticalScrollbarSize": 5,
	"editor.scrollBeyondLastLine": false,
	"editor.smoothScrolling": true,
	"editor.tabCompletion": "on",
	"editor.tabSize": 2,
	"editor.cursorBlinking": "phase",
	"editor.cursorSmoothCaretAnimation": "on",
	"editor.formatOnSave": true,
	"files.trimFinalNewlines": true,
	"files.trimTrailingWhitespace": true,
	"editor.lineHeight": 23,
	"editor.bracketPairColorization.independentColorPoolPerBracketType": true,
	"workbench.cloudChanges.continueOn": "off",
	"workbench.commandPalette.history": 10,
	"workbench.enableExperiments": false,
	"workbench.list.smoothScrolling": true,
	"workbench.localHistory.enabled": false,
	"workbench.welcomePage.walkthroughs.openOnInstall": false,
	"workbench.iconTheme": "material-icon-theme",
	"breadcrumbs.filePath": "off",
	"breadcrumbs.icons": false,
	"workbench.editor.enablePreviewFromCodeNavigation": true,
	"workbench.editor.tabSizing": "shrink",
	"zenMode.centerLayout": false,
	"window.menuBarVisibility": "toggle",
	"window.title": "${dirty}${activeEditorShort}${separator}${profileName}${separator}${appName}",
	"explorer.compactFolders": false,
	"update.showReleaseNotes": false,
	"css.format.spaceAroundSelectorSeparator": true,
	"css.hover.references": false,
	"css.lint.duplicateProperties": "warning",
	"html.hover.references": false,
	"importCost.fontStyle": "italic",
	"npm.packageManager": "npm",
	"prettier.useTabs": true,
	"simpleBrowser.focusLockIndicator.enabled": false,
	"[svelte]": {
		"editor.defaultFormatter": "svelte.svelte-vscode"
	},
	"[prisma]": {
		"editor.defaultFormatter": "Prisma.prisma"
	},
	"svelte.plugin.svelte.format.config.svelteStrictMode": true,
	"thunder-client.httpVersion": "HTTP/2",
	"windicss.enableCodeFolding": true,
	"windicss.sortOnSave": true,
	"editor.wordWrap": "on",
	"javascript.suggest.completeFunctionCalls": true,
	"search.exclude": {
		"**/.git": true
	}
}

