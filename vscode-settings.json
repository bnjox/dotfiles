{
	"telemetry.telemetryLevel": "off",
	"files.autoSave": "onFocusChange",
	"editor.tabSize": 2,
	"workbench.iconTheme": "material-icon-theme",
	"workbench.colorTheme": "Community Material Theme Palenight High Contrast",
	"security.workspace.trust.banner": "never",
	"security.workspace.trust.emptyWindow": false,
	"security.workspace.trust.enabled": false,
	"security.workspace.trust.startupPrompt": "never",
	"[dart]": {
		"editor.formatOnType": true,
		"editor.selectionHighlight": false,
		"editor.suggest.snippetsPreventQuickSuggestions": false,
		"editor.tabCompletion": "onlySnippets",
		"editor.wordBasedSuggestions": false
	},
	"editor.bracketPairColorization.independentColorPoolPerBracketType": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.guides.bracketPairs": true,
	"editor.fontLigatures": true,
	"editor.fontSize": 14,
	"editor.fontFamily": "'JetBrains Mono', 'Droid Sans Mono', 'monospace'",
	"editor.formatOnSave": true,
	"files.insertFinalNewline": true,
	"workbench.preferredDarkColorTheme": "Community Material Theme Palenight High Contrast",
	"workbench.preferredHighContrastColorTheme": "Community Material Theme Palenight",
	"workbench.tree.indent": 11,
	"breadcrumbs.enabled": false,
	"workbench.editor.labelFormat": "short",
	"window.menuBarVisibility": "toggle",
	"editor.minimap.enabled": false,
	"workbench.activityBar.visible": false,
	"terminal.integrated.cursorStyle": "line",
	"css.lint.duplicateProperties": "warning",
	"dart.flutterHotReloadOnSave": "allIfDirty",
	"html.hover.references": false,
	"importCost.fontStyle": "italic",
	"svelte.ask-to-enable-ts-plugin": false,
	"path-intellisense.extensionOnImport": true,
	"path-intellisense.ignoreTsConfigBaseUrl": true,
	"prettier.bracketSameLine": true,
	"prettier.useTabs": true,
	"tailwindCSS.classAttributes": ["class", "className", "ngClass", "classes"],
	"editor.tabCompletion": "on",
	"[prisma]": {
		"editor.defaultFormatter": "Prisma.prisma"
	},
	"editor.smoothScrolling": true,
	"editor.cursorBlinking": "phase",
	"editor.cursorSmoothCaretAnimation": "on",
	"workbench.settings.enableNaturalLanguageSearch": false,
	"explorer.compactFolders": false,
	"editor.linkedEditing": true,
	"html.format.extraLiners": "body",
	"editor.quickSuggestions": {
		"strings": "on"
	},
	"windicss.sortOnSave": true,
	"windicss.enableCodeFolding": true,
	"explorer.fileNesting.expand": false,
	"explorer.expandSingleFolderWorkspaces": false,
	"search.exclude": {
		"**/node_modules": true,
		"**/*.code-search": true,
		"**/.git": true,
		"**/.github": true,
		"**/package-lock.json": true
	},
	"terminal.integrated.cursorBlinking": true,
	"terminal.integrated.fontWeight": "300",
	"terminal.integrated.persistentSessionReviveProcess": "never",
	"files.eol": "\n",
	"extensions.autoUpdate": "onlyEnabledExtensions",
	"workbench.tree.expandMode": "singleClick",
	"workbench.list.smoothScrolling": true,
	"editor.accessibilitySupport": "off",
	"editor.find.addExtraSpaceOnTop": false,
	"editor.inlineSuggest.enabled": true,
	"prettier.printWidth": 200,
	"editor.multiCursorModifier": "ctrlCmd",
	"eslint.codeActionsOnSave.mode": "problems",
	"eslint.format.enable": true,
	"typescript.preferences.quoteStyle": "double",
	"javascript.preferences.quoteStyle": "double",
	"editor.autoClosingDelete": "always",
	"editor.autoClosingQuotes": "always",
	"path-intellisense.mappings": {
		"$lib": "${workspaceFolder}/src/lib"
	},
	"editor.scrollbar.verticalScrollbarSize": 10,
	"editor.scrollbar.horizontalScrollbarSize": 10,
	"terminal.integrated.smoothScrolling": true,
	"[markdown]": {
		"editor.wordWrap": "on"
	},
	"workbench.tree.renderIndentGuides": "always",
	"window.title": "${dirty}${activeEditorMedium}",
	"files.trimFinalNewlines": true,
	"files.trimTrailingWhitespace": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"css.hover.references": false,
	"less.hover.references": false,
	"scss.hover.references": false,
	"javascript.preferences.importModuleSpecifierEnding": "js",
	"typescript.preferences.importModuleSpecifierEnding": "js",
	"editor.autoClosingBrackets": "always",
	"css.lint.float": "warning",
	"less.lint.float": "warning",
	"scss.lint.float": "warning"
	// "js/ts.implicitProjectConfig.checkJs": true
	// "js/ts.implicitProjectConfig.checkJs": true
}
