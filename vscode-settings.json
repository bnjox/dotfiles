{
	"workbench.colorTheme": "Community Material Theme Palenight High Contrast",
	"editor.foldingImportsByDefault": true,
	"editor.scrollBeyondLastLine": false,
	"workbench.cloudChanges.continueOn": "off",
	"workbench.commandPalette.history": 10,
	"workbench.enableExperiments": false,
	"workbench.localHistory.enabled": false,
	"workbench.welcomePage.walkthroughs.openOnInstall": false,
	"breadcrumbs.filePath": "off",
	"breadcrumbs.icons": false,
	"workbench.editor.enablePreviewFromCodeNavigation": true,
	"workbench.editor.tabSizing": "shrink",
	"zenMode.centerLayout": false,
	// "window.title": "${dirty}${activeEditorShort}${separator}${profileName}${separator}${appName}",
	"update.showReleaseNotes": false,
	"css.format.spaceAroundSelectorSeparator": true,
	"npm.packageManager": "npm",
	"simpleBrowser.focusLockIndicator.enabled": false,
	"thunder-client.httpVersion": "HTTP/2",
	"editor.wordWrap": "on",
	"javascript.suggest.completeFunctionCalls": true,

	// =========== VSCODE
	"telemetry.telemetryLevel": "off",
	"files.autoSave": "onFocusChange",
	"security.workspace.trust.banner": "never",
	"security.workspace.trust.emptyWindow": false,
	"security.workspace.trust.enabled": false,
	"security.workspace.trust.startupPrompt": "never",
	"explorer.decorations.badges": false,
	"editor.tabSize": 2,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.fontLigatures": true,
	"editor.fontSize": 13.6,
	"editor.fontFamily": "'Fira Code', 'Droid Sans Mono', 'monospace'",
	"editor.formatOnSave": true,
	"editor.lineHeight": 24,
	"editor.tabCompletion": "on",
	"editor.smoothScrolling": true,
	"editor.cursorBlinking": "phase",
	"editor.cursorSmoothCaretAnimation": "on",
	"editor.quickSuggestions": {
		"strings": "on"
	},
	"editor.accessibilitySupport": "off",
	"editor.find.addExtraSpaceOnTop": false,
	"editor.inlineSuggest.enabled": true,
	"editor.autoClosingDelete": "always",
	"editor.autoClosingQuotes": "always",
	"editor.scrollbar.verticalScrollbarSize": 10,
	"editor.scrollbar.horizontalScrollbarSize": 10,
	"editor.minimap.enabled": false,
	"editor.linkedEditing": true,
	"files.insertFinalNewline": true,
	"workbench.preferredDarkColorTheme": "Community Material Theme Palenight High Contrast",
	"workbench.preferredHighContrastColorTheme": "Community Material Theme Palenight",
	"workbench.tree.indent": 11,
	"breadcrumbs.enabled": false,
	"workbench.editor.labelFormat": "short",
	"window.": "toggle",
	"workbench.activityBar.visible": false,
	"terminal.integrated.cursorStyle": "line",
	"css.lint.duplicateProperties": "warning",
	"html.hover.references": false,
	"importCost.fontStyle": "italic",
	"svelte.ask-to-enable-ts-plugin": false,
	// "path-intellisense.extensionOnImport": true,
	// "path-intellisense.ignoreTsConfigBaseUrl": true,
	"prettier.bracketSameLine": true,
	"prettier.useTabs": true,
	// "tailwindCSS.classAttributes": ["class", "className", "ngClass"],
	"[prisma]": {
		"editor.defaultFormatter": "Prisma.prisma"
	},
	"workbench.settings.enableNaturalLanguageSearch": false,
	"explorer.compactFolders": false,
	"html.format.extraLiners": "body",
	"windicss.sortOnSave": true,
	"windicss.enableCodeFolding": true,
	"explorer.fileNesting.expand": false,
	"explorer.expandSingleFolderWorkspaces": false,
	"search.exclude": {
		"**/node_modules": true,
		"**/*.code-search": true,
		"**/.git": true,
		"**/.github": true,
		"**/package-lock.json": true
	},
	"terminal.integrated.cursorBlinking": true,
	"terminal.integrated.fontWeight": "300",
	"terminal.integrated.persistentSessionReviveProcess": "never",
	"files.eol": "\n",
	"extensions.autoUpdate": "onlyEnabledExtensions",
	"workbench.tree.expandMode": "singleClick",
	"workbench.list.smoothScrolling": true,
	"typescript.preferences.quoteStyle": "double",
	"javascript.preferences.quoteStyle": "double",
	"terminal.integrated.smoothScrolling": true,
	"[markdown]": {
		"editor.wordWrap": "on"
	},
	"workbench.tree.renderIndentGuides": "always",
	"window.title": "${dirty}${activeEditorMedium}",
	"files.trimTrailingWhitespace": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"css.hover.references": false,
	"javascript.preferences.importModuleSpecifierEnding": "js",
	"typescript.preferences.importModuleSpecifierEnding": "js",
	"css.lint.float": "warning",
	"less.lint.float": "warning",
	"scss.lint.float": "warning",
	"javascript.format.semicolons": "insert",
	"[svelte]": {
		"editor.defaultFormatter": "svelte.svelte-vscode"
	},
	"prettier.printWidth": 100,
	"git.openRepositoryInParentFolders": "never",
	"svelte.plugin.svelte.format.config.printWidth": 100,
	"svelte.plugin.svelte.format.config.svelteAllowShorthand": true,
	"html.format.templating": true,
	"workbench.iconTheme": "material-icon-theme",
	"material-icon-theme.folders.color": "#90a4ae",
	"material-icon-theme.folders.theme": "specific",
	"js/ts.implicitProjectConfig.checkJs": true
}

